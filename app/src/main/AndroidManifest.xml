<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devup.opointdoacai.opointdoacaiserver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.OrderHistory"></activity>
        <activity android:name=".Activity.OrderStatus3" />
        <activity android:name=".Activity.OrderStatus2" />
        <activity android:name=".Activity.OrderPannel" />
        <activity android:name=".Activity.RegisterSizes" />
        <activity android:name=".Activity.RegisterFruitSalads" />
        <activity android:name=".Activity.RegisterVitamins" />
        <activity android:name=".Activity.RegisterJuices" />
        <activity android:name=".Activity.RegisterTops" />
        <activity android:name=".Activity.RegisterComps" />
        <activity android:name=".Activity.RegisterProducts" />
        <activity android:name=".Activity.MainActivity" />

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/default_notification_channel_id" />

        <activity android:name=".Activity.Administrador" />
        <activity android:name=".Activity.Login" />
        <activity android:name=".Activity.Register" />
        <activity android:name=".Activity.OrderStatus" />
        <activity android:name=".Activity.Authentication" />
        <activity android:name=".Activity.PhoneAuth">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".Service.MyFirebaseIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".Service.MyFirebaseMessaging">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".Activity.OrderDetail" />
    </application>

</manifest>